{% trans_default_domain 'FOSUserBundle' %}
{% form_theme form 'P5indicatoriUserBundle:Default:form_div_layout.html.twig' %}

<form class="well form-horizontal login-form go-in-center" action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
    <legend><div class="">{{ 'Register form'|trans }}</div></legend>
     {{ form_row(form.email) }}
     {{ form_row(form.username) }}
     {{ form_row(form.plainPassword) }}
     {{ form_rest(form) }}
    <div class="control-group go-in-center" >
        <input class="btn" type="submit" value="{{ 'Register'|trans }}" />
    </div>
</form>
